{
    "name": "starlit/db-migration",
    "type": "library",
    "description": "A lightweight database migrator.",
    "keywords": ["database", "MySQL", "PDO", "ORM", "migration"],
    "homepage": "http://github.com/starweb/starlit-db-migration",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Starweb AB"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-pdo_mysql": "*",
        "starlit/db": "^0.12.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {"Starlit\\Db\\Migration\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {"Starlit\\Db\\Migration\\Tests\\": "src"}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
